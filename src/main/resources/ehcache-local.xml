<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="defaultCache">
	<diskStore path="java.io.tmpdir/ehcache/"/>
	<!-- DefaultCache setting. -->
	<defaultCache maxElementsInMemory="1000" eternal="false" timeToLiveSeconds="0" overflowToDisk="false" />	
	<!-- 此缓存不知有何用？暂时给注释掉20150118
	   timeToLiveSeconds="86400"   7200
	<cache name="frontCache" maxElementsInMemory="1000" eternal="false" timeToLiveSeconds="3600" overflowToDisk="false" />	 
	-->		
	<cache name="sysCache"   maxElementsInMemory="10000" eternal="false" overflowToDisk="false" timeToLiveSeconds="86400"/> 
	<cache name="ttCache"  maxElementsInMemory="10000" eternal="false" overflowToDisk="false" timeToLiveSeconds="86400" memoryStoreEvictionPolicy="LFU"/>   		
</ehcache>
